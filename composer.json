{
    "name": "beelab/memory-repository-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle to use in-memory repositories",
    "keywords": ["symfony", "doctrine", "testing"],
    "homepage": "http://bee-lab.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        },
        {
            "name": "Mauro D'Alatri",
            "email": "mauro.dalatri@bee-lab.net"
        }
    ],
    "require": {
        "doctrine/orm": "2.5.*@dev",
        "doctrine/dbal": "2.5.*@dev",
        "doctrine/doctrine-bundle": "~1.3@beta"
    },
    "require-dev":{
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Beelab\\MemoryRepositoryBundle": "" }
    },
    "target-dir": "Beelab/MemoryRepositoryBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
